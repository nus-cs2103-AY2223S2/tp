@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model <<Rectangle>>{
Class "<<interface>>\nReadOnlyShop" as ReadOnlyShop
Class "<<interface>>\nReadOnlyUserPrefs" as ReadOnlyUserPrefs
Class "<<interface>>\nModel" as Model
Class Shop
Class ModelManager
Class UserPrefs

Class UniqueXList
Class X
Class Service
Class PartMap
'Class Part
Class ServiceList
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

Shop .up.|> ReadOnlyShop

ModelManager .up.|> Model
Model .right.> ReadOnlyUserPrefs
Model .left.> ReadOnlyShop
ModelManager -left-> "1" Shop
ModelManager -right-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

Shop *--> "1" UniqueXList
Shop *--> "1" ServiceList
UniqueXList --> "~* all" X
ModelManager -->"~* filtered" X

Shop *--> "1" PartMap
'PartMap --> "~* all" Part
ModelManager -->"1" PartMap

ServiceList --> "~* all" Service
ModelManager -->"~* filtered" Service

@enduml
