@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model <<Rectangle>>{
Class "<<interface>>\nReadOnlyAddressBook" as ReadOnlyAddressBook
Class "<<interface>>\nReadOnlyUserPrefs" as ReadOnlyUserPrefs
Class "<<interface>>\nModel" as Model

Class AddressBook
Class ModelManager
Class UserPrefs

Class UniquePersonList
Class Person
Class Address
Class Email
Class Name
Class Telegram
Class Tag
Class Remark
Class Photo
Class Performance

Class UniqueTutorialList
Class Tutorial

Class UniqueLabList
Class Lab

Class UniqueConsultationList
Class Consultation
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

AddressBook .up.|> ReadOnlyAddressBook

ModelManager .up.|> Model
Model .right.> ReadOnlyUserPrefs
Model .left.> ReadOnlyAddressBook
ModelManager -left-> "1" AddressBook
ModelManager --> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

AddressBook *--> "1" UniquePersonList

UniquePersonList --> "*" Person
Person *--> "1" Name
Person *--> "1" Telegram
Person *--> "1" Email
Person *--> "1" Address
Person *--> "1" Performance
Person *--> "1" Remark
Person *--> "1" Photo

Name -[hidden]right-> Telegram
Telegram -[hidden]right-> Address
Address -[hidden]right-> Email

AddressBook *--> "1" UniqueTutorialList
AddressBook *--> "1" UniqueLabList
AddressBook *--> "1" UniqueConsultationList

UniqueConsultationList --> "*" Consultation
UniqueTutorialList --> "*" Tutorial
UniqueLabList --> "*" Lab

Consultation --> "*" Person
Tutorial --> "*" Person
Lab --> "*" Person

ModelManager ------>"~* filtered" Person
@enduml
