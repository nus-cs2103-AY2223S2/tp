<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.StackPane?>

<?import javafx.scene.chart.PieChart?>

<StackPane fx:id="detailedContent" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets top="10" right="10" bottom="10" left="10" />
    </padding>

    <VBox id="studentHomeworkContainer" nodeOrientation="LEFT_TO_RIGHT" prefHeight="300.0" prefWidth="500.0">
        <Label fx:id="name" alignment="TOP_LEFT" styleClass="studentNameLabel" text="Full Name" />
        <Label fx:id="listName" alignment="TOP_LEFT" text="List Name" />
        <StackPane fx:id="homeworkListPlaceholder" prefHeight="400.0" prefWidth="300.0" />
        <PieChart fx:id="homeworkPieChart" />
    </VBox>
</StackPane>
