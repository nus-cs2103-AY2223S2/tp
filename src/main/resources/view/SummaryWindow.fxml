<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.VBox?>
<?import javafx.stage.Stage?>

<fx:root resizable="false" title="Summary" type="javafx.stage.Stage"
         xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
    <icons>
        <Image url="@/images/summary_icon.png" />
    </icons>
    <scene>
        <Scene fill="#fffffb">
            <stylesheets>
                <URL value="@SummaryWindow.css" />
            </stylesheets>
         <VBox minWidth="-Infinity" prefHeight="500.0" prefWidth="700.0" fx:id="summaryMessageContainer">
            <children>
                    <Label fx:id="countMessage" text="Label" />
                    <Label fx:id="tagMessage" text="Label" />
                    <Label fx:id="companies" text="Label" />
                    <Label fx:id="potentialEarnings" text="Label" />
            </children>
            <padding>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
         </VBox>
        </Scene>
    </scene>
</fx:root>
