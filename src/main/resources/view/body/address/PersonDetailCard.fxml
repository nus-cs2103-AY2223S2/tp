<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="5" styleClass="pane-with-rounded-corner-accented, pa"
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <Label fx:id="title" styleClass="label-h6" text="\$title" wrapText="true" />
    <Label fx:id="body" styleClass="label-p" text="\$body" wrapText="true" />
</VBox>
