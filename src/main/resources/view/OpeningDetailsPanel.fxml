<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.TextFlow?>

<ScrollPane fitToWidth="true">
    <content>
        <VBox fx:id="openingDetailsPanel" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
            <Label fx:id="position" styleClass="details-header" text="\$position" />
            <Label fx:id="company" text="\$company" />
            <Label fx:id="status" text="\$status" />
            <FlowPane fx:id="dates" />
            <Label fx:id="email" text="\$email" />
            <Label fx:id="remark" text="\$remark" />
        </VBox>
    </content>
</ScrollPane>
