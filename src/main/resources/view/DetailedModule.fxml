<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<VBox alignment="TOP_CENTER" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <StackPane VBox.vgrow="NEVER">
         <children>
             <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#f0f0f0" height="30.0" strokeType="INSIDE" style="-fx-arc-height: 20; -fx-arc-width: 20;" width="300.0" />
            <HBox alignment="CENTER">
               <children>
                  <StackPane maxHeight="50" minHeight="50" minWidth="100.0">
                     <children>
                          <Label contentDisplay="CENTER" text="Contact" textAlignment="CENTER" />
                     </children>
                  </StackPane>
                  <StackPane maxHeight="50" minHeight="50" minWidth="100.0">
                     <children>
                        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#ffc09f" height="30.0" strokeType="INSIDE" style="-fx-arc-height: 20; -fx-arc-width: 20;" width="100.0" />
                        <Label contentDisplay="CENTER" text="Module" textAlignment="CENTER" />
                     </children>
                  </StackPane>
                  <StackPane maxHeight="50" minHeight="50" minWidth="100.0">
                     <children>
                        <Label contentDisplay="CENTER" text="Skill" textAlignment="CENTER" />
                     </children>
                  </StackPane>
               </children>
            </HBox>
         </children>
      </StackPane>
        <ListView fx:id="moduleListView" maxWidth="330" minHeight="340.0" VBox.vgrow="NEVER" />
    </children>
   <padding>
      <Insets bottom="10.0" />
   </padding>
</VBox>
