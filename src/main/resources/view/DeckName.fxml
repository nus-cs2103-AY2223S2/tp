<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="deckNamePane" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1">
    <GridPane alignment="CENTER" HBox.hgrow="ALWAYS">
        <VBox alignment="CENTER" prefHeight="85" GridPane.columnIndex="0">
            <padding>
                <Insets bottom="5" left="15" right="5" top="5" />
            </padding>
            <Label fx:id="title" styleClass="cell_big_label" wrapText="true" />
            <Label fx:id="description" styleClass="cell_big_label" />
        </VBox>
    </GridPane>
</HBox>
