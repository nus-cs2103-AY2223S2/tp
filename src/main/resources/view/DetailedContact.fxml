<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane>
         <children>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#ebebeb" height="30.0" strokeType="INSIDE" style="-fx-arc-height: 20; -fx-arc-width: 20;" width="300.0" />
            <HBox alignment="CENTER">
               <children>
                  <StackPane maxHeight="50" minHeight="50" minWidth="100.0">
                     <children>
                        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#ffc9de" height="30.0" strokeType="INSIDE" style="-fx-arc-height: 20; -fx-arc-width: 20;" width="100.0" />
                        <Label contentDisplay="CENTER" text="Contact" textAlignment="CENTER" />
                     </children>
                  </StackPane>
                  <StackPane maxHeight="50" minHeight="50" minWidth="100.0">
                     <children>
                        <Label contentDisplay="CENTER" text="Module" textAlignment="CENTER" />
                     </children>
                  </StackPane>
                  <StackPane maxHeight="50" minHeight="50" minWidth="100.0">
                     <children>
                        <Label contentDisplay="CENTER" text="Skill" textAlignment="CENTER" />
                     </children>
                  </StackPane>
               </children>
            </HBox>
         </children>
      </StackPane>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label text="GitHub ID:" />
            <Label fx:id="github" text="\\\$git_hub_link" />
            <StackPane prefHeight="20.0" prefWidth="300.0" />
            <Label text="Email Address:" />
            <Label fx:id="email" text="\\\$email" />
            <StackPane layoutX="10.0" layoutY="38.0" prefHeight="20.0" prefWidth="300.0" />
            <Label text="LinkedIn Profile:" />
            <Label fx:id="linkedin" text="\\\$linkedin" />
         </children></VBox>
   </children>
</VBox>
