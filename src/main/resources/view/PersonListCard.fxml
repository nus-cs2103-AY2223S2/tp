<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
    <HBox>
      <children>
   <HBox alignment="CENTER_LEFT" spacing="5">
     <Label fx:id="id" styleClass="cell_big_label">
       <minWidth>
         <!-- Ensures that the label text is never truncated -->
         <Region fx:constant="USE_PREF_SIZE" />
       </minWidth>
     </Label>
     <Label fx:id="name" styleClass="cell_big_label" text="\$first">
            <HBox.margin>
               <Insets />
            </HBox.margin>
         </Label>
      <Label fx:id="gender" graphicTextGap="6.0" styleClass="cell_big_label" text="\$gender">
               <padding>
                  <Insets left="5.0" right="10.0" />
               </padding></Label>
      <Label fx:id="status" contentDisplay="RIGHT" styleClass="cell_lead_status_label" text="\$lead_status">
            <padding>
               <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
            </padding>
         </Label>
            <HBox.margin>
               <Insets />
            </HBox.margin>
   </HBox>
      </children>
      <VBox.margin>
         <Insets bottom="5.0" />
      </VBox.margin></HBox>
<FlowPane fx:id="tags" alignment="CENTER_LEFT" prefWrapLength="100.0">
      <VBox.margin>
         <Insets />
      </VBox.margin>
   </FlowPane>
<HBox id="cardPane" fx:id="cardPane">
 <VBox alignment="CENTER_LEFT" minHeight="75.0" minWidth="300.0">
         <GridPane prefWidth="214.0">
           <columnConstraints>
             <ColumnConstraints hgrow="SOMETIMES" maxWidth="146.0" minWidth="10.0" prefWidth="68.0" />
             <ColumnConstraints hgrow="SOMETIMES" maxWidth="244.0" minWidth="10.0" prefWidth="232.0" />
           </columnConstraints>
           <rowConstraints>
               <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
               <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
               <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
               <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
           </rowConstraints>
             <children>
                 <Label alignment="TOP_LEFT" styleClass="cell_small_label" text="Phone" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.valignment="TOP" />
                 <Label alignment="TOP_LEFT" centerShape="false" styleClass="cell_small_label" text="Address" GridPane.columnIndex="0" GridPane.rowIndex="1" GridPane.valignment="TOP">
                  <GridPane.margin>
                     <Insets />
                  </GridPane.margin></Label>
                 <Label alignment="TOP_LEFT" styleClass="cell_small_label" text="Email" GridPane.columnIndex="0" GridPane.rowIndex="2" GridPane.valignment="TOP" />
                 <Label fx:id="phone" alignment="TOP_LEFT" styleClass="cell_small_label" text="\$phone" GridPane.columnIndex="1" GridPane.rowIndex="0" GridPane.valignment="TOP" />
                 <Label fx:id="address" alignment="TOP_LEFT" styleClass="cell_small_label" text="\$address" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.valignment="TOP" />
                 <Label fx:id="email" alignment="TOP_LEFT" styleClass="cell_small_label" text="\$email" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="2" GridPane.valignment="TOP" />

             </children>
         </GridPane>
     <Label fx:id="remark" styleClass="cell_small_label" text="\$remark" />
 </VBox>
    <VBox minHeight="40.0" HBox.hgrow="ALWAYS">
        <GridPane prefWidth="214.0">
        <columnConstraints>
            <ColumnConstraints hgrow="NEVER" maxWidth="146.0" minWidth="10.0" prefWidth="83.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="250.0" minWidth="10.0" prefWidth="131.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
        </rowConstraints>
            <children>
                <Label alignment="TOP_LEFT" styleClass="cell_small_label" text="Location" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.valignment="TOP" />
                <Label alignment="TOP_LEFT" styleClass="cell_small_label" text="Company" GridPane.columnIndex="0" GridPane.rowIndex="1" GridPane.valignment="TOP" />
                <Label alignment="TOP_LEFT" styleClass="cell_small_label" text="Occupation" GridPane.columnIndex="0" GridPane.rowIndex="2" GridPane.valignment="TOP" />
                <Label alignment="TOP_LEFT" styleClass="cell_small_label" text="Job Title" GridPane.columnIndex="0" GridPane.rowIndex="3" GridPane.valignment="TOP" />
                <Label fx:id="locationn" alignment="TOP_LEFT" styleClass="cell_small_label" text="\$locationn" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="0" GridPane.valignment="TOP" />
                <Label fx:id="company" alignment="TOP_LEFT" styleClass="cell_small_label" text="\$company" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.valignment="TOP" />
                <Label fx:id="occupation" alignment="TOP_LEFT" styleClass="cell_small_label" text="\$occupation" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="2" GridPane.valignment="TOP" />
                <Label fx:id="jobTitle" alignment="TOP_LEFT" styleClass="cell_small_label" text="\$jobTitle" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="3" GridPane.valignment="TOP" />
            </children>
        </GridPane>
   </VBox>
      <VBox.margin>
         <Insets />
      </VBox.margin>
      <padding>
         <Insets bottom="5.0" right="5.0" top="5.0" />
      </padding>
</HBox>
   <padding>
      <Insets bottom="5.0" left="15.0" right="5.0" top="5.0" />
   </padding>
</VBox>
