<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="stack-pane" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets left="5.0" right="5.0" top="5.0" />
   </padding>
   <children>
      <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" StackPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" maxWidth="400.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox alignment="CENTER">
                     <children>
                        <Label alignment="CENTER" styleClass="stats_header" text="SPENT THIS MONTH">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin></Label>
                        <Label fx:id="monthlySpending" alignment="CENTER" styleClass="statDetail" text="\$monthlySpending" />
                     </children>
                  </VBox>
                  <Region maxWidth="1.7976931348623157E308" minWidth="30.0" HBox.hgrow="ALWAYS" />
                  <VBox alignment="CENTER">
                     <children>
                        <Label alignment="CENTER" styleClass="stats_header" text="SPENT THIS WEEK">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin></Label>
                        <Label fx:id="weeklySpending" alignment="CENTER" styleClass="statDetail" text="\$weeklySpending" />
                     </children>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER" maxWidth="400.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox alignment="CENTER">
                     <children>
                        <Label alignment="CENTER" styleClass="stats_header" text="MONTHLY REMAINING">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin></Label>
                        <Label fx:id="monthlyRemaining" alignment="CENTER" styleClass="statDetail" text="\$monthlyRemaining" />
                        <HBox fx:id="monthlyChangeBackground" alignment="CENTER" maxWidth="130.0" prefWidth="130.0" styleClass="change_indicator_background_positive">
                           <children>
                              <Label fx:id="monthlySign" text="\$sign">
                                 <styleClass>
                                    <String fx:value="change_indicator" />
                                    <String fx:value="positive_change_indicator" />
                                 </styleClass>
                              </Label>
                              <Label fx:id="monthlyChange" alignment="CENTER" minWidth="-Infinity" text="\$monthlyChange">
                                 <styleClass>
                                    <String fx:value="change_indicator" />
                                    <String fx:value="positive_change_indicator" />
                                 </styleClass>
                              </Label>
                           </children>
                           <VBox.margin>
                              <Insets top="15.0" />
                           </VBox.margin>
                        </HBox>
                     </children>
                  </VBox>
                  <Region minWidth="30.0" HBox.hgrow="SOMETIMES" />
                  <VBox alignment="CENTER">
                     <children>
                        <Label alignment="CENTER" styleClass="stats_header" text="WEEKLY REMAINING">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin>
                        </Label>
                        <Label fx:id="weeklyRemaining" alignment="CENTER" styleClass="statDetail" text="\$weeklyRemaining">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </Label>
                        <HBox fx:id="weeklyChangeBackground" alignment="CENTER" maxWidth="130.0" prefWidth="130.0" styleClass="change_indicator_background_positive">
                           <children>
                              <Label fx:id="weeklySign" text="\$sign">
                                 <styleClass>
                                    <String fx:value="change_indicator" />
                                    <String fx:value="positive_change_indicator" />
                                 </styleClass>
                              </Label>
                              <Label fx:id="weeklyChange" alignment="CENTER" minWidth="-Infinity" text="\$weeklyChange">
                                 <styleClass>
                                    <String fx:value="change_indicator" />
                                    <String fx:value="positive_change_indicator" />
                                 </styleClass>
                              </Label>
                           </children>
                           <VBox.margin>
                              <Insets top="15.0" />
                           </VBox.margin>
                        </HBox>
                     </children>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_RIGHT" maxWidth="400.0" styleClass="stat_box" VBox.vgrow="ALWAYS">
               <children>
                  <VBox alignment="CENTER">
                     <children>
                        <Label alignment="CENTER" styleClass="stats_header" text="TOTAL SPENT">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin></Label>
                        <Label fx:id="totalSpending" alignment="CENTER" styleClass="statDetail" text="\$totalSpending" />
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="15.0" right="20.0" top="10.0" />
                     </padding>
                  </VBox>
                  <Region minWidth="30.0" HBox.hgrow="SOMETIMES" />
                  <VBox alignment="CENTER">
                     <children>
                        <Label alignment="CENTER" styleClass="stats_header" text="BUDGET UTILISED">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin></Label>
                        <Label fx:id="budgetPercentage" alignment="CENTER" styleClass="statDetail" text="\$budgetPercentage" />
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="20.0" right="15.0" top="10.0" />
                     </padding>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_LEFT" maxWidth="400.0" VBox.vgrow="ALWAYS">
               <children>
                  <Label fx:id="budgetAdvice" styleClass="stats_advice" text="\$budgetAdvice" />
               </children>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </HBox>
         </children>
         <StackPane.margin>
            <Insets bottom="15.0" left="25.0" right="5.0" top="25.0" />
         </StackPane.margin>
      </VBox>
   </children>
</StackPane>
