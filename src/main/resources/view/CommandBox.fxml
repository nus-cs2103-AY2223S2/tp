<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.StackPane?>

<StackPane styleClass="stack-pane" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
  <TextField fx:id="commandTextField" maxHeight="50.0" minHeight="50.0" onAction="#handleCommandEntered" prefHeight="50.0" promptText="Enter command here...">
      <StackPane.margin>
         <Insets left="5.0" />
      </StackPane.margin></TextField>
   <padding>
      <Insets left="5.0" right="5.0" top="5.0" />
   </padding>
</StackPane>
