<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label fx:id="resultsCount" text="\$resultsCount">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
               <font>
                  <Font name="Helvetica" size="13.0" />
               </font>
               <styleClass>
                  <String fx:value="details_header" />
                  <String fx:value="results_count" />
               </styleClass>
            </Label>
            <Label styleClass="details_header" text="entries found" />
            <Region maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
            <Label fx:id="dateLabel" styleClass="details_header" text="Date:">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="dateFilter" alignment="CENTER_RIGHT" text="\$dateFilter">
               <styleClass>
                  <String fx:value="details_header" />
                  <String fx:value="date_filter" />
               </styleClass></Label>
         </children>
      </HBox>
   </children>
</StackPane>
