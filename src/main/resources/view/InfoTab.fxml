<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<VBox id="infoPane" fx:id="infoPane" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
         <children>
            <StackPane fx:id="profilePic" prefHeight="33.0" prefWidth="200.0" />
            <Label fx:id="name" alignment="CENTER" text="Label" />
            <Label fx:id="identity" alignment="CENTER" text="Label" />
         </children>
      </VBox>
      <StackPane fx:id="detailedInfoPlaceholder" prefHeight="150.0" prefWidth="200.0" />
   </children>
</VBox>
